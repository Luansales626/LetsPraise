{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "src",
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
